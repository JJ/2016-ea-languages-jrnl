###############################################################################

CFLAGS = -march=native -O3 -std=gnu11 -Wall

###############################################################################

SRC = $(wildcard *.c)
EXE = $(basename $(SRC))

###############################################################################

default: $(EXE)

all: $(EXE)
	for i in $^; do echo $$i; ./$$i; done

clean:
	$(RM) -rf $(EXE) $(TXT)

###############################################################################

.PHONY: all clean default
.NOEXPORT:

###############################################################################
