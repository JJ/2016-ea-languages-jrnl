###############################################################################

CFLAGS = -march=native -O3 -std=gnu11 -Wall

###############################################################################

SRC = $(wildcard *.c)
EXE = $(basename $(SRC))

###############################################################################

all: $(EXE)

clean:
	$(RM) -rf $(EXE) $(TXT)

###############################################################################

.PHONY: all clean default
.NOEXPORT:

###############################################################################
