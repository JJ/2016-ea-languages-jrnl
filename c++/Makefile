###############################################################################

CXXFLAGS = -g -march=native -O3 -std=c++11 -Wall

###############################################################################

SRC = $(wildcard *.cc)
EXE = $(basename $(SRC))

###############################################################################

all:	$(EXE)

clean:
	$(RM) -rf $(EXE) core.* perf.* *~

###############################################################################

.PHONY: all clean
.NOEXPORT:

###############################################################################
